    "lint": "next lint",
    "lint:md": "remark ."
  },
  "dependencies": {
    "@tailwindcss/typography": "0.4.1",
    "classnames": "2.3.1",
    "date-fns": "2.22.1",
    "debug": "4.3.2",
    "gray-matter": "4.0.3",
