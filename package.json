{
  "name": "kcblog",
  "display": "KCBlog",
  "version": "0.4.0",
  "description": "Personal blogs for collect and share purpose",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kamontat/blog"
  },
  "scripts": {
