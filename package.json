    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "postcss": "8.3.5",
    "prettier": "2.3.2",
    "remark-cli": "9.0.0",
    "remark-preset-lint-recommended": "5.0.0",
    "tailwindcss": "2.2.4",
    "typescript": "4.3.5"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "space"
