    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-autolink-headings": "5.1.0",
    "rehype-highlight": "4.1.0",
    "rehype-react": "6.2.1",
    "rehype-sanitize": "4.0.0",
    "rehype-stringify": "8.0.0",
    "remark": "13.0.0",
    "remark-directive": "1.0.1",
    "remark-external-links": "8.0.0",
    "remark-gfm": "1.0.0",
    "remark-images": "2.0.0",
    "remark-rehype": "8.1.0",
    "remark-slug": "6.0.0",
